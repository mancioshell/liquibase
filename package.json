{
  "name": "liquibase",
  "packageManager": "yarn@4.9.1",
  "scripts": {
    "dev": "ts-node src/server.ts",
    "prebuild": "rm -rf dist",
    "build": "tsc",
    "typeorm": "ts-node-esm -r tsconfig-paths/register ./node_modules/typeorm/cli.js -d ./src/migrations.ts",
    "migration:generate": "npm run typeorm migration:generate ./src/migrations/CreateDatabase",
    "migration:run": "npm run typeorm migration:run",
    "test": "vitest  --config ./vite.config.ts run"
  },
  "dependencies": {
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "pg": "^8.16.3",
    "reflect-metadata": "^0.2.2",
    "typeorm": "^0.3.27"
  },
  "devDependencies": {
    "@swc/core": "1.13.5",
    "@testcontainers/postgresql": "^11.7.1",
    "@types/dotenv": "^8.2.3",
    "@types/express": "^5.0.3",
    "@types/node": "^24.7.0",
    "@types/pg": "^8.15.5",
    "liquibase": "^4.28.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.3",
    "unplugin-swc": "^1.5.7",
    "vitest": "^3.2.4"
  }
}
